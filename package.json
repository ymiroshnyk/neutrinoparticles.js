{
  "name": "neutrinoparticles.js",
  "version": "1.0.4",
  "description": "Javascript runtimes for neutrinoparticles",
  "main": "dist/neutrinoparticles.umd.js",
  "keywords": [
    "neutrinoparticles",
    "particles"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymiroshnyk/neutrinoparticles.js.git"
  },
  "author": "Yurii Miroshnyk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ymiroshnyk/neutrinoparticles.js/issues"
  },
  "homepage": "https://github.com/ymiroshnyk/neutrinoparticles.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.3",
    "@babel/node": "^7.6.3",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "scripts": {
    "clean": "./node_modules/.bin/rimraf ./dist",
    "build": "rollup -c -m",
    "cleanbuild": "npm run clean && npm run build",
    "dev": "npm run build -- -w",
    "pretest": "npm run build",
    "test": "npm run just-test",
    "just-test": "",
    "predocs": "",
    "docs": "",
    "publish:patch": "npm whoami && npm run cleanbuild && VERSION=$(npm version patch) && grep -q $VERSION CHANGES.md && npm publish",
    "publish:minor": "npm whoami && npm run cleanbuild && VERSION=$(npm version minor) && grep -q $VERSION CHANGES.md && npm publish",
    "publish:major": "npm whoami && npm run cleanbuild && VERSION=$(npm version major) && grep -q $VERSION CHANGES.md && npm publish",
    "postpublish": "git push && git push --tags"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "retainLines": true
  },
  "files": [
    "bin",
    "dist"
  ]
}
